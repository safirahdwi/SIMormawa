@model Ormawa.ViewModels.DaftarPublikasiOrmawaViewModel;

@{
    ViewBag.Title = "Publikasi";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        @ViewBag.Title
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">@ViewBag.Title</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">
                        Membuat Publikasi
                    </h3>
                    <div>
                        <form method="get" action="/Daftarpublikasi/Add">
                            <br />
                            <button type="submit" class="btn btn-success btn-xs"> Add <span class="glyphicon glyphicon-plus"></span></button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>