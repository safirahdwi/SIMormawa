@model Ormawa.ViewModels.OrganisasiOrmawaVM
@{
    ViewData["Title"] = "Detail";
}

<h2>Detail</h2>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-body">
                    <div class="dataTables_wrapper form-inline dt-bootstrap" id="example1_wrapper">
                        <div class="row">
                            <div class="col-sm-12">
                                <table aria-describedby="example1_info" role="grid" id="example1" class="table table-bordered table-striped dataTable">
                                    <thead>
                                        <tr>
                                            <th>
                                                @Html.DisplayNameFor(model => model.Mahasiswa)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.OrganisasiOrmawa)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.JabatanOrmawa)
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var i in Model.GetDetails)
                                        {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => i.Mahasiswa)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => i.OrganisasiOrmawa)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => i.JabatanOrmawa)
                                            </td>
                                        </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
